public class RECHNEN {
	public RECHNEN()
	{
	}
	public int fibonacci(int n)
	{
		if (n<1)
		{
			System.out.println("Fehler");
			return 0;
		}
		
		if (n==1)
		{
			return 0;
		}
		
		if (n==2)
		{
			return 1;
		}
		
		else
			
		{
			return fibonacci(n-1)+fibonacci(n-2);
		}
	}
	
	public int fakultaet(int n)
	{
		if (n<1)
		{
			System.out.println("Fehler!");
		}
		if (n==1)
		{
			return 1;
		}
		else
		{
			return n+fakultaet(n-1);
		}
	}

}
